      <PasswordField fx:id="pass1" alignment="CENTER" layoutX="182.0" layoutY="121.0" prefHeight="25.0" prefWidth="236.0" promptText="Enter New Password" />
      <PasswordField fx:id="pass2" alignment="CENTER" layoutX="182.0" layoutY="177.0" prefHeight="25.0" prefWidth="236.0" promptText="Re-Enter New Password" />
      <Label layoutX="201.0" layoutY="25.0" text="Change Password">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label fx:id="confirmation" layoutX="212.0" layoutY="280.0" prefHeight="35.0" prefWidth="176.0" />
   </children>
</AnchorPane>
