<AnchorPane id="AnchorPane" prefHeight="547.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TeamCoach.ViewApplicationsController">
