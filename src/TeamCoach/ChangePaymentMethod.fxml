      <TextField fx:id="accNo" layoutX="186.0" layoutY="276.0" prefHeight="25.0" prefWidth="232.0" promptText="Account No." />
      <Button layoutX="262.0" layoutY="317.0" mnemonicParsing="false" onAction="#saveBtnOnClick" prefHeight="35.0" prefWidth="73.0" text="Save" />
      <Label fx:id="currentPayment" alignment="TOP_CENTER" layoutX="111.0" layoutY="67.0" prefHeight="72.0" prefWidth="383.0" />
